package com.cpmg.day2;

public class phone {
	@Override
	public String toString() {
		return "phone [price=" + price + ", brandname=" + brandname + ", color=" + color + ", storage=" + storage
				+ ", screenSize=" + screenSize + "]";
	}
	private int price;
	private String brandname;
	private String color; 
	private int storage;
	private float screenSize;
	phone(int price, String brandName, String color, int storage, float screensize)
	{
		this.price=price; 
		this.brandname=brandname;
		this.color=color;
		this.storage=storage;
		this.screenSize=screenSize;
	}
	public static void main(String[] args) {
		phone p1=new phone("abc","hfu","djf","ldk");
		phone p2=new phone("gjr","jif","jpdj","fkg");
		phone p3=new phone("djf","ldk","jpdj","fkg");
		System.out.println(p1.toString());
	}

}
