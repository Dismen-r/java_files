package com.cpmg.day2;
class phone1{
	private int price;
	private String brandname;
	private String color; 
	private int storage;
	private float screenSize;
	phone(int price, String brandName, String color, int storage, float screensize)
	{
		this.price=price; 
		this.brandname=brandname;
		this.color=color;
		this.storage=storage;
		this.screenSize=screenSize;
	}
}
class laptop{
	private int price;
	private String brandname;
	private String color; 
	private int storage;
	private float screenSize;
	
}
class tablet{

	private int price;
	public tablet(int price, String brandname, String color, int storage, float screenSize) {
		super();
		this.price = price;
		this.brandname = brandname;
		this.color = color;
		this.storage = storage;
		this.screenSize = screenSize;
	}
	private String brandname;
	private String color; 
	private int storage;
	private float screenSize;
}
